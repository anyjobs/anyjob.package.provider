{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Knife": {
    "StaticFiles": {
      "/files": {
        "FolderPath": "/anyjob/packs",
        "ServeUnknownFileTypes": true
      }
    }
  },
  "FileProvider": {
    "UrlFormat": "/files/{0}"
  },
  "AllowedHosts": "*"
}